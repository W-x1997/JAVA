<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
  
<package name="default" extends="struts-default" namespace="/">
		
		<action name="login" class="Actions.actions" method="login">
			<result name="success">/loginSuccess.jsp</result>
			<result name="error">/index.jsp</result>
		</action>
		
		
		<action name="add" class="Actions.actions" method="add">
			<result name="success">/add_Success.jsp</result>
		</action>
		
		<action name="query" class="Actions.actions" method="query">
		  <result>/querySuccess.jsp</result>
		</action>
		
		<action name="delete" class="Actions.actions" method="delete">
		  <result>/querySuccess.jsp</result>
		</action>
		
		<action name="check" class="Actions.actions" method="check">
		  <result>/update.jsp</result>
		</action>
		
		<action name="update" class="Actions.actions" method="update">
		  <result>/querySuccess.jsp</result>
		</action>
		
</package>
</struts>    
